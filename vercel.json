{
  "functions": {
    "src/main/java/Mypackage/*.java": {
      "runtime": "vercel-community-java-runtime"
    }
  },
  "routes": [
    {
      "src": "/MyServlet",
      "dest": "src/main/java/Mypackage/MyServlet.java"
    },
    {
      "src": "/(.*)",
      "dest": "src/main/webapp/$1"
    }
  ]
}